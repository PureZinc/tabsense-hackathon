{
    "manifest_version": 3,
    "name": "TabSense",
    "version": "1.0",
    "description": "Keep track of your Chrome experience.",
    "action": {
      "default_popup": "html/popup.html"
    },
    "permissions": [
        "storage", 
        "activeTab", 
        "scripting",
        "tabs"
    ],
    "background": {
        "service_worker": "service-worker.js"
    }
}
